{
  "name": "ewasm-kernel",
  "version": "0.0.0",
  "description": "This is a JS prototype of the eWASM kernal.",
  "scripts": {
    "lint": "standard",
    "test": "node --harmony --expose-wasm ./tests/interfaceRunner.js",
    "build": "node ./tests/buildTests.js && ./tools/wabt/out/clang/Debug/wat2wasm ./wasm/interface.wast -o ./wasm/interface.wasm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewasm/ewasm-kernel.git"
  },
  "bugs": {
    "url": "https://github.com/ewasm/ewasm-kernel/issues"
  },
  "homepage": "https://github.com/ewasm/ewasm-kernel",
  "keywords": [
    "ethereum",
    "webassembly",
    "wasm",
    "ewasm"
  ],
  "author": "mjbecze <mjbecze@gmail.com>",
  "contributors": "Alex Beregszaszi <alex@rtfs.hu>",
  "license": "MPL-2.0",
  "devDependencies": {
    "standard": "*",
    "tape": "^4.5.1"
  },
  "standard": {
    "ignore": [
      "/tools/"
    ],
    "globals": [
      "WebAssembly"
    ]
  },
  "dependencies": {
    "bn.js": "^4.11.6",
    "ethereumjs-block": "^1.2.2",
    "ethereumjs-tx": "^1.1.2",
    "ethereumjs-util": "^5.0.0",
    "loglevel": "1.5.1",
    "merkle-trie": "0.0.0",
    "minimist": "^1.2.0"
  }
}
